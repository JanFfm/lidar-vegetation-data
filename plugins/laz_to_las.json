{
    "tool_name": "LazToLas",
    "exe": "laz_to_las",
    "short_description": "This tool converts one or more LAZ files into the LAS format",
    "toolbox": "LiDAR Tools",
    "license": "Proprietary",
    "example": ">> .*EXE_NAME -r=LazToLas -i=file.laz -o=outfile.las",
    "parameters": [
        {
            "name": "Input LAZ File",
            "flags": ["-i", "--input"],
            "description": "Name of the input LAZ files (leave blank to use all LAZ files in WorkingDirectory.",
            "parameter_type": {"ExistingFile":"Lidar"},
            "default_value": null,
            "optional": true
        },
        {
            "name": "Output LAS File",
            "flags": ["-o", "--output"],
            "description": "Output LAS file (including extension).",
            "parameter_type": {"NewFile":"Raster"},
            "default_value": null,
            "optional": true
        }
    ]
}