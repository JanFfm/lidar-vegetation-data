{
    "tool_name": "LasToLaz",
    "exe": "las_to_laz",
    "short_description": "This tool converts one or more LAS files into the LAZ format",
    "toolbox": "LiDAR Tools",
    "license": "Proprietary",
    "example": ">> .*EXE_NAME -r=LazToLas -i=file.laz -o=outfile.las",
    "parameters": [
        {
            "name": "Input LAS File",
            "flags": ["-i", "--input"],
            "description": "Name of the input LAS files (leave blank to use all LAS files in WorkingDirectory.",
            "parameter_type": {"ExistingFile":"Lidar"},
            "default_value": null,
            "optional": true
        },
        {
            "name": "Output LAZ File",
            "flags": ["-o", "--output"],
            "description": "Output LAZ file (including extension).",
            "parameter_type": {"NewFile":"Raster"},
            "default_value": null,
            "optional": true
        }
    ]
}